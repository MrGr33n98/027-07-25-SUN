{
    "name": "solar-connect-nextjs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:performance": "jest --testPathPatterns=performance.test.ts --runInBand",
        "test:auth-performance": "jest --testPathPatterns=auth-performance.test.ts --runInBand",
        "test:security": "jest --testPathPatterns=__tests__/security --runInBand --verbose",
        "test:security:attack-vectors": "jest --testPathPatterns=attack-vector-tests.test.ts --runInBand",
        "test:security:brute-force": "jest --testPathPatterns=brute-force-simulation.test.ts --runInBand",
        "test:security:timing": "jest --testPathPatterns=timing-attack-resistance.test.ts --runInBand",
        "test:security:penetration": "jest --testPathPatterns=penetration-testing.test.ts --runInBand",
        "security:assessment": "ts-node __tests__/security/security-test-runner.ts",
        "benchmark": "node scripts/run-performance-benchmarks.js",
        "benchmark:report": "node scripts/run-performance-benchmarks.js --output benchmark-report.txt",
        "benchmark:json": "node scripts/run-performance-benchmarks.js --format json --output benchmark-report.json",
        "benchmark:html": "node scripts/run-performance-benchmarks.js --format html --output benchmark-report.html",
        "benchmark:auth": "node scripts/run-auth-performance-benchmarks.js",
        "benchmark:auth:report": "node scripts/run-auth-performance-benchmarks.js --output auth-benchmark-report.txt",
        "benchmark:auth:json": "node scripts/run-auth-performance-benchmarks.js --format json --output auth-benchmark-report.json",
        "benchmark:auth:html": "node scripts/run-auth-performance-benchmarks.js --format html --output auth-benchmark-report.html",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "db:generate": "prisma generate",
        "db:seed": "ts-node scripts/seed.ts",
        "db:reset": "prisma db push --force-reset && npm run db:seed",
        "db:optimize": "node scripts/run-database-optimization.js",
        "db:optimize:dry-run": "node scripts/run-database-optimization.js --dry-run",
        "db:optimize:indexes": "node scripts/run-database-optimization.js --indexes-only",
        "migrate:users": "ts-node scripts/migrate-existing-users.ts",
        "migrate:users:dry-run": "ts-node scripts/migrate-existing-users.ts --dry-run",
        "migrate:security": "ts-node scripts/migrate-security-data.ts",
        "migrate:security:dry-run": "ts-node scripts/migrate-security-data.ts --dry-run",
        "deploy:auth": "ts-node scripts/deploy-auth-system.ts",
        "deploy:auth:staging": "ts-node scripts/deploy-auth-system.ts --env=staging",
        "deploy:auth:production": "ts-node scripts/deploy-auth-system.ts --env=production",
        "deploy:auth:dry-run": "ts-node scripts/deploy-auth-system.ts --dry-run",
        "deploy:rollback": "ts-node scripts/deploy-auth-system.ts rollback",
        "flags": "ts-node scripts/manage-feature-flags.ts",
        "flags:list": "ts-node scripts/manage-feature-flags.ts list",
        "backup": "ts-node scripts/backup-recovery.ts",
        "backup:create": "ts-node scripts/backup-recovery.ts create --type=full --encrypt --compress",
        "backup:auth": "ts-node scripts/backup-recovery.ts create --type=auth-only --encrypt --compress",
        "backup:restore": "ts-node scripts/backup-recovery.ts restore",
        "backup:list": "ts-node scripts/backup-recovery.ts list",
        "validate:deployment": "ts-node scripts/validate-deployment.ts",
        "validate:deployment:verbose": "ts-node scripts/validate-deployment.ts --verbose"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.10.0",
        "@hookform/resolvers": "^3.10.0",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.7.1",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-select": "^2.0.0",
        "@radix-ui/react-slider": "^1.3.5",
        "@radix-ui/react-slot": "^1.0.2",
        "@radix-ui/react-tabs": "^1.0.4",
        "@tanstack/react-query": "^5.17.1",
        "@types/bcryptjs": "^2.4.6",
        "@uploadthing/react": "^7.3.2",
        "@upstash/redis": "^1.35.1",
        "bcryptjs": "^3.0.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "lucide-react": "^0.303.0",
        "next": "14.0.4",
        "next-auth": "^4.24.11",
        "react": "^18",
        "react-day-picker": "^9.8.1",
        "react-dom": "^18",
        "react-dropzone": "^14.3.8",
        "react-hook-form": "^7.48.2",
        "resend": "^4.7.0",
        "sonner": "^2.0.6",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "uploadthing": "^7.7.3",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.0.4",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "jest-mock-extended": "^4.0.0",
        "postcss": "^8",
        "prisma": "^5.7.1",
        "tailwindcss": "^3.3.0",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    }
}